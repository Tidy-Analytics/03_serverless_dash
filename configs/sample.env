# Sample client configuration for 03_serverless_dash
# Copy this file to {CLIENT_NAME}.env and update values

# Basic client information
CLIENT_NAME=xxxx
ADMIN_EMAIL={MASTER_ADMIN_EMAIL}
ADMIN_IP={MASTER_ADMIN_IP}
ADMIN_OBJ={MASTER_ADMIN_OBJECT_ID}
APP_OWNER=client.admin@example.com
APP_OWNER_NAME="Client Admin Name"
ORG_NAME="Client Organization Name"
TEMP_PW="TempPassword123!"

# Custom settings for 03_serverless_dash
CUSTOM_DOMAIN=xxxx.tidyanalytics.com
LOCATION=CentralUS
GH_TOKEN={YOUR_GITHUB_TOKEN}
REPO_NAME=Tidy-Analytics/03_serverless_dash
REPO_URL=https://github.com/Tidy-Analytics/03_serverless_dash

# Azure configuration (from master provisioner)
AZURE_CLIENT_ID={MASTER_PROVISIONER_CLIENT_ID}
AZURE_TENANT_ID={AZURE_TENANT_ID}
AZURE_SUBSCRIPTION_ID={AZURE_SUBSCRIPTION_ID}

# Master resource group for provisioner identity (not client RG)
RESOURCE_GROUP={MASTER_RESOURCE_GROUP}
IDENTITY_NAME=client-provisioner
CONTAINER_STORE={CONTAINER_REGISTRY_RESOURCE_ID}